package com.gtpl.qa.pages;

import java.util.List;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

import com.gtpl.qa.base.GTPLBase;

public class NewCustomerPage extends GTPLBase{
	
	
	@FindBy(name="name")
	private WebElement txtCustomerName;
	
	@FindBy(xpath="//td/input[@type='radio']")
	private List<WebElement> radioGender;
	
	@FindBy(id="dob")
	private WebElement txtDOBDay;
	@FindBy(id="dob")
	private WebElement txtDOBMonth;
	@FindBy(id="dob")
	private WebElement txtDOBYear;
	
	
	@FindBy(name="addr")
	private WebElement txtCustomerAddress;
	
	@FindBy(name="city")
	private WebElement txtCustomerCity;
	
	@FindBy(name="state")
	private WebElement txtCustomerState;
	
	@FindBy(name="pinno")
	private WebElement txtCustomerPinCode;
	
	@FindBy(name="telephoneno")
	private WebElement txtCustomerPhone;
	
	@FindBy(name="emailid")
	private WebElement txtCustomerEmail;
	@FindBy(name="password")
	private WebElement txtCustomerPassword;
	
	@FindBy(name="sub")
	private WebElement txtCustomerSubmit;
	@FindBy(name="res")
	private WebElement txtCustomerReset;
	
	
	

}
